def isFloat(string):
    try:
        value = (float(string))
        if value >= 0:
            return True
    except:
        return False

while True:
    change = input("Change owed:")
    if isFloat(change) == True:
        change = float(change)
        break
    else:
        continue

cents = round(int(change * 100))
count = 0

while True:
    if cents >= 25:
        cents -= 25
        count += 1
    elif cents >= 10:
        cents -= 10
        count += 1
    elif cents >= 5:
        cents -= 5
        count += 1
    elif cents >= 1:
        cents -= 1
        count += 1
    else:
        break

print(count)
