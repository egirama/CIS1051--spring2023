#This program creates an analog clock using a turtle

import turtle

bob = turtle.Turtle()
bob.shape("turtle")
bob.speed(7)

bob.penup()

for size in range(12):
    bob.forward(130)
    bob.pendown()
    bob.forward(15)
    bob.penup()
    bob.forward(15)
    bob.stamp()
    bob.forward(-160)
    bob.right(30)
