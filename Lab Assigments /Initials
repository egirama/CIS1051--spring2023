#This program draws my initials

import turtle
bob = turtle.Turtle()

bob.speed(2)
bob.penup()
bob.goto(-50,100)
bob.pendown()

bob.forward(50)
bob.penup()
bob.goto(-50,100)
bob.pendown()

bob.right(90)
bob.forward(50)
bob.left(90)
bob.forward(50)

bob.goto(-50,50)
bob.right(90)
bob.forward(50)
bob.left(90)
bob.forward(50)

bob.penup()
bob.goto(50,0)
bob.pendown()
bob.left(90)
bob.forward(100)

bob.right(90)
bob.circle(-25,180)

bob.left(120)
bob.forward(50)
